{
    "files.associations": {
        "*.expect": "shell"
    },
    "editor.formatOnSave": true,
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        },
    },
    "go.testFlags": [
        // Sometimes the Vscode built-in test runner caches results even if we add more .expect files
        "-count=1"
    ]
}